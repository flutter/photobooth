rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /{folder}/{imageId} {
    	allow read: if imageId.matches(".*\\.png") || imageId.matches(".*\\.jpg") || imageId.matches(".*\\.jpeg") || imageId.matches(".*\\.svg");
      allow write: if request.auth != null;
    }
  }
}
